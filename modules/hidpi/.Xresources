Xft.dpi:       192
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! special
*.foreground:   #b3b3b3
*.background:   #000000
*.cursorColor:  #b3b3b3

! black
*.color0:       #2a2a2a
*.color8:       #555555

! red
*.color1:       #cc5555
*.color9:       #ff5555

! green
*.color2:       #55cc55
*.color10:      #55ff55

! yellow
*.color3:       #d3bf00
*.color11:      #f3d64e

! blue
*.color4:       #5555cc
*.color12:      #5555ff

! magenta
*.color5:       #b449be
*.color13:      #ff55ff

! cyan
*.color6:       #7acaca
*.color14:      #55ffff

! white
*.color7:       #cccccc
*.color15:      #ffffff

Xcursor.theme: Adwaita
Xcursor.size:  32

XTerm*renderFont: true
XTerm*faceName: Ubuntu Mono
XTerm*faceSize: 32
